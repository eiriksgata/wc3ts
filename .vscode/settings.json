{
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnSave": true
  },
  "eslint.run": "onType",
  "eslint.format.enable": true,
  "editor.rulers": [100],
  "editor.tabSize": 2,
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true
}
